{
	"version": "0.2.0",
	"configurations": [
    {
      "cwd": "${workspaceFolder}",
      "executable": "build/zephyr/zephyr.elf",
      "name": "Debug with PyOCD",
      "request": "launch",
      "type": "cortex-debug",
      "serverArgs": [
        "--target", "STM32F407ZGTx",     
      ],
      "runToEntryPoint": "main",
      "showDevDebugOutput": "none",
      "servertype": "pyocd"
    }
	]
}